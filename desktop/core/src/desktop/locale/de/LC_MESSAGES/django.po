msgstr "Sie müssen ein Superuser sein."

#: src/desktop/views.py:97
msgid "No logs found!"
msgstr "Keine Protokolle gefunden!"

#: src/desktop/views.py:138
msgid "No logs found."
msgstr "Keine Protokolle gefunden."

#: src/desktop/auth/forms.py:28
msgid "Username"
msgstr "Benutzername"

#: src/desktop/auth/forms.py:29 src/desktop/auth/forms.py:38
msgid "Password"
